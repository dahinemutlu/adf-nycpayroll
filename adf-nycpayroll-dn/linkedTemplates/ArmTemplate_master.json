{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-nycpayroll-dn"
		},
		"ls_db_nycpayroll_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_db_nycpayroll'"
		},
		"ls_sanycpayrolldn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sanycpayrolldn'"
		},
		"ls_sanycpayrollsynapsedn_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sanycpayrollsynapsedn'"
		},
		"ls_sanycpayrolldn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sanycpayrolldn.dfs.core.windows.net/"
		},
		"ls_sanycpayrollsynapsedn_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sanycpayrollsynapsedn.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-nycpayroll-dn_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_db_nycpayroll_connectionString": {
						"value": "[parameters('ls_db_nycpayroll_connectionString')]"
					},
					"ls_sanycpayrolldn_accountKey": {
						"value": "[parameters('ls_sanycpayrolldn_accountKey')]"
					},
					"ls_sanycpayrollsynapsedn_accountKey": {
						"value": "[parameters('ls_sanycpayrollsynapsedn_accountKey')]"
					},
					"ls_sanycpayrolldn_properties_typeProperties_url": {
						"value": "[parameters('ls_sanycpayrolldn_properties_typeProperties_url')]"
					},
					"ls_sanycpayrollsynapsedn_properties_typeProperties_url": {
						"value": "[parameters('ls_sanycpayrollsynapsedn_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-nycpayroll-dn_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-nycpayroll-dn_ArmTemplate_0')]"
			]
		}
	]
}